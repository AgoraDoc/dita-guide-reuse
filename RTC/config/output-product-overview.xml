<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="https://www.dita-ot.org/project">
  <context name="Live Streaming Android" id="product-overview">
    <input href="../_Product-Overview.ditamap"/>
    <profile>
      <ditaval href="filter-live.ditaval"/>
    </profile>
  </context>
  <deliverable name="Markdown">
    <context idref="product-overview"/>
    <output href="../../out/markdown/product-overview/"/>
    <publication transtype="markdown">
      <param name="clean.temp" value="yes"/>
      <param name="force-unique" value="true"/>
    </publication>
  </deliverable>
  <deliverable name="HTML">
    <context idref="product-overview"/>
    <output href="../../out/html/product-overview/"/>
    <publication transtype="html5">
      <param name="nav-toc" value="full"/>
      <param name="clean.temp" value="yes"/>
      <param name="force-unique" value="true"/>
    </publication>
  </deliverable>
</project>
